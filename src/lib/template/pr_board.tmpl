<% c++ #include "controller/prboard.h" %>
<% skin my_skin %>
<% view pr_board uses Content::prBoard extends board %>

<% template captchaField(std::string position) %>
<% if ( "Top" == position ) %>
    <div id="googleCaptcha">
        <script type="text/javascript" src="//codecha.org/api/challenge?k=<%= codechaPublicKey %>"> </script>
    </div>
<% end %>
<% end template %>

<% end view %>
<% end skin %>
